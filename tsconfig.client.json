{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "dom"],
    "types": [
      "node",
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules"]
    },
    "outDir": "dist",
    "rootDir": "./src",
    "strict": true,
    "noImplicitAny": false,
    "strictPropertyInitialization": false,
    "jsx": "react"
  },
  "include": [
    "src/"
  ],
  "exclude": [
    "node_modules"
  ],
}